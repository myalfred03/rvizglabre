        </visual> -->

    </link>

    <joint name="joint3-tool0" type="fixed">
      <parent link="z_axis_link"/>
      <child link="tool0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>

      <link name="tool0"/>

