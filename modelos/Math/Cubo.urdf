<?xml version="1.0" ?>
<robot name="example">
<link name="tool0"/>
<link name="base_link" />

<link name="link_1">
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <box size="0.5 0.5 0.5"/>
    </geometry>
  </visual>
</link>

<joint name="joint_1" type="revolute">
  <parent link="base_link" />
  <child link="link_1" />
  <limit effort="30" velocity="1.0" lower="-1.570795" upper="1.570795" />
  <origin xyz="0 0 0" rpy="0 0 0" />
  <axis xyz="1 0 0" />
</joint>

<joint name="joint_t" type="fixed">
  <parent link="link_1" />
  <child link="tool0" />
  <origin xyz="0 0 0" rpy="0 0 0" />
</joint>

</robot>

