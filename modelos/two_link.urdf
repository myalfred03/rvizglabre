<?xml version="1.0" ?>
<robot name="2dof" xmlns:xacro="http://ros.org/wiki/xacro">

<link name="tool0"/>

<link name="base_link" />

<link name="link_1">
  <visual>
    <origin xyz="0 0 0.5" rpy="0 0 0"/>
    <geometry>
      <box size="0.1 0.1 1"/>
    </geometry>
  </visual>
</link>
<link name="link_2">
  <visual>
    <origin xyz="0 0 0.25" rpy="0 0 0"/>
    <geometry>
      <box size="0.1 0.1 0.5"/>
    </geometry>
  </visual>
</link>

<joint name="joint_1" type="revolute">
  <parent link="base_link" />
  <child link="link_1" />
  <limit effort="30" velocity="1.0" lower="-1.570795" upper="1.570795" />
  <origin xyz="0 0 0" rpy="0 0 0" />
  <axis xyz="1 0 0" />
</joint>
<joint name="joint_2" type="revolute">
  <parent link="link_1" />
  <child link="link_2" />
  <limit effort="30" velocity="1.0" lower="-1.570795" upper="1.570795" />
  <origin xyz="0 0 1" rpy="0 0 0" />
  <axis xyz="1 0 0" />
</joint>

<joint name="joint_3" type="revolute">
  <parent link="link_2" />
  <child link="tool0" />
  <limit effort="30" velocity="1.0" lower="-0.1" upper="0.1" />
  <origin xyz="0 0 0.5" rpy="0 0 0" />
  <axis xyz="0 0 1" />
</joint>
</robot>

